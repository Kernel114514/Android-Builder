[
  {
    "kernelSource": {
      "name": "katom",
      "repo": "https://github.com/Kernel114514/android_kernel_xiaomi_mt6885",
      "branch": "lineage-21",
      "device": "atom",
      "defconfig": "vendor/atom_user_defconfig"
    },
    "toolchains": [
      {
        "name": "proton-clang",
        "repo": "https://github.com/Kernel114514/proton-clang",
        "branch": "master",
        "binaryEnv": ["./bin"]
      }
    ],
    "enableCcache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "clang",
      "externalCommands": {
        "CROSS_COMPILE": "proton-clang/bin/aarch64-linux-gnu-",
        "CROSS_COMPILE_ARM32": "proton-clang/bin/arm-linux-gnueabi-",
        "CLANG_TRIPLE": "aarch64-linux-gnu-"
      }
    },
    "AnyKernel3": {
      "use": false,
      "release": true
    },
    "enableKernelSU": false,
    "enableLXC": false
  }
]
